{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "outDir": "./dist/",
    "preserveConstEnums": true,
    "removeComments": true,
    "jsx": "preserve",
    "sourceMap": true,
    "target": "ES6",
    "resolveJsonModule": true
  }
}
